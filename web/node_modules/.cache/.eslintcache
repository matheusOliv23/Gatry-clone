      <input\r\n            id=\"title\"\r\n            name=\"title\"\r\n            type=\"text\"\r\n            onChange={onChange}\r\n            value={values.title}\r\n          />\r\n        </div>\r\n        <div className=\"promotion-form__group\">\r\n          <label htmlFor=\"url\">Link</label>\r\n          <input\r\n            id=\"url\"\r\n            name=\"url\"\r\n            type=\"url\"\r\n            onChange={onChange}\r\n            value={values.url}\r\n          />\r\n        </div>\r\n        <div className=\"promotion-form__group\">\r\n          <label htmlFor=\"imageUrl\">Imagem (URL)</label>\r\n          <input\r\n            id=\"imageUrl\"\r\n            name=\"imageUrl\"\r\n            type=\"text\"\r\n            onChange={onChange}\r\n            value={values.imageUrl}\r\n          />\r\n        </div>\r\n        <div className=\"promotion-form__group\">\r\n          <label htmlFor=\"price\">Preço</label>\r\n          <input\r\n            id=\"price\"\r\n            name=\"price\"\r\n            type=\"number\"\r\n            onChange={onChange}\r\n            value={values.price}\r\n          />\r\n        </div>\r\n        <div>\r\n          <button type=\"submit\">Salvar</button>\r\n        </div>\r\n      </form>\r\n    </UIContainer>\r\n  )\r\n}\r\n\r\nexport default PromotionForm\r\n","E:\\React\\react-gatry\\web\\src\\components\\utils\\useApi.js",[],"E:\\React\\react-gatry\\web\\src\\components\\UI\\Modal\\Modal.js",[],"E:\\React\\react-gatry\\web\\src\\components\\utils\\useDebouncePromise.js",[],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":32,"column":6,"nodeType":"72","endLine":32,"endColumn":14,"suggestions":"73"},{"ruleId":"74","severity":1,"message":"75","line":7,"column":5,"nodeType":"76","endLine":7,"endColumn":71},{"ruleId":"70","severity":1,"message":"71","line":39,"column":6,"nodeType":"72","endLine":39,"endColumn":10,"suggestions":"77"},"no